{
  "env": {
    "jasmine": true,
    "node": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "extends": ["plugin:prettier/recommended"],
  "plugins": ["import", "prettier"],
  "rules": {
    "func-names": [2, "never"],
    "import/no-extraneous-dependencies": [2, { "devDependencies": true }],
    "no-underscore-dangle": [2, { "allow": ["__moduleBundler"] }],
    "prettier/prettier": "error"
  }
}
