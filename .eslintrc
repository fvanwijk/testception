{
    "env": {
        "jasmine": true,
        "node": true,
        "browser": true
    },
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "comma-dangle": [2, "never"],
        "func-names": [2, "never"],
        "import/no-extraneous-dependencies": [2, {"devDependencies": true }],
        "no-underscore-dangle": [2, { "allow": ["__moduleBundler"] }]
    }
}
